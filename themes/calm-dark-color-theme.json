	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"comment.block.preprocessor",
				"comment.documentation",
				"comment.block.documentation",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#666666"
			}
		},
		{
			"name": "Errors",
			"scope": [
				"entity.name.exception",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword",
				"keyword.operator",
				"keyword.operator.expression",
				"storage"
			],
			"settings": {
				"foreground": "#a0a0a0"
			}
		},
		{
			"name": "Type declarations and references",
			"scope": [
				"meta.class",
				"meta.return-type",
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"storage.type",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
				"support.type",
				"support.class",
				"support.function",
				"entity.other.inherited-class",
				"entity.name.class",
				"entity.name.type",
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#888888"
			}
		},
		{
			"name": "Variables and parameter names",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable",
				"variable.parameter",
				"variable.css",
				"variable.scss",
				"variable.other.less"
			],
			"settings": {
				"foreground": "#888888"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#a0a0a0"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Constants, numbers, characters",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant.language",
				"constant.other.symbol",
				"support.constant",
				"variable.language",
				"constant"
			],
			"settings": {
				"foreground": "#888888"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#888888"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#bb9977"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#bb9977"
			}
		},
		{
			"name": "HTML",
			"scope": [
				"constant.character.entity",
				"entity.other.attribute-name.html",
				"entity.name.tag",
				"meta.tag",
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html",
				"punctuation.definition.entity",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#666666"
			}
		},
		{
			"name": "Selector",
			"scope": [
				"entity.name.selector",
				"meta.selector"
			],
			"settings": {
				"foreground": "#888888"
			}
		},
		{
			"name": "CSS property value",
			"scope": [
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color",
				"support.type.vendored.property-name",
				"support.type.property-name",
				"meta.structure.dictionary.key.python",
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#a0a0a0"
			}
		},
		{
			"name": "Markup: Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#cccccc"
			}
		},
		{
			"name": "Markup: Deletion",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#cccccc"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Error",
			"scope": "markup.error",
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"name": "Markup: Insertion",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#cccccc"
			}
		},
		{
			"name": "Markup: Link",
			"scope": "meta.link",
			"settings": {
				"foreground": "#99bbcc"
			}
		},
		{
			"name": "Markup: Output",
			"scope": [
				"markup.output",
				"markup.raw"
			],
			"settings": {
				"foreground": "#666666"
			}
		},
		{
			"name": "Markup: Prompt",
			"scope": "markup.prompt",
			"settings": {
				"foreground": "#666666"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Traceback",
			"scope": "markup.traceback",
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#666666"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#666666"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#888888"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"foreground": "#cccccc"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"foreground": "#cccccc"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"foreground": "#cccccc"
			}
		}
	]
}