		"tab.activeBorderTop": "#444444",
		"tab.inactiveModifiedBorder": "#444444",
		"tab.unfocusedActiveBorderTop": "#444444",
		"tab.inactiveBackground": "#303030",
    "textBlockQuote.background": "#303030",
    "textPreformat.foreground": "#888888",
