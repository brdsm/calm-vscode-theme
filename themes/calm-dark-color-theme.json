    "panelSectionHeader.background": "#303030",
		"sideBar.background": "#303030",
		"sideBar.border": "#444444",
		"sideBarSectionHeader.background": "#303030",
		"statusBar.background": "#303030",
		"statusBar.foreground": "#dddddd",
		"statusBar.border": "#444444",
    "statusBar.noFolderBackground": "#cfc8ff",
		"statusBarItem.remoteBackground": "#303030",
		"statusBarItem.remoteForeground": "#000000",
    "tab.border": "#444444",
