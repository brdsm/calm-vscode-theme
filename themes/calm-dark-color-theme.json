		"terminal.tab.activeBorder": "#6699ff",
		"terminal.background": "#282828",
		"terminal.foreground": "#cccccc",
		"terminal.selectionBackground": "#383838",
		"terminal.ansiWhite": "#efefef",
		"terminal.ansiBlack": "#000000",
		"terminal.ansiBlue": "#5577cc",
		"terminal.ansiCyan": "#77aaaa",
		"terminal.ansiGreen": "#709966",
		"terminal.ansiMagenta": "#bb88cc",
		"terminal.ansiRed": "#cc6068",
		"terminal.ansiYellow": "#bb9977",
		"terminal.ansiBrightWhite": "#ffffff",
		"terminal.ansiBrightBlack": "#a8a8a8",
		"terminal.ansiBrightBlue": "#aaccee",
		"terminal.ansiBrightCyan": "#bbdddd",
		"terminal.ansiBrightGreen": "#bbddb8",
		"terminal.ansiBrightMagenta": "#ddbbee",
		"terminal.ansiBrightRed": "#ee9098",
		"terminal.ansiBrightYellow": "#eeddaa"
