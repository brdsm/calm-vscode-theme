		"focusBorder": "#888888",
		"list.activeSelectionBackground": "#444444",
		"list.activeSelectionForeground": "#eeeeee",
		"list.hoverBackground": "#444444",
		"list.inactiveSelectionBackground": "#303030",
		"quickInput.background": "#303030",
		"quickInput.foreground": "#cccccc",
		"quickInputList.focusBackground": "#666666",
		"quickInputList.focusForeground": "#eeeeee",
		"panel.background": "#282828",
    "panelTitle.activeBorder": "#444444",
