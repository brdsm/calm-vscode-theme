		"editorBracketMatch.background": "#383838ee",
		"editorIndentGuide.background": "#383838ee",
		"editorIndentGuide.activeBackground": "#444444",
		"editorLineNumber.foreground": "#383838",
		"editorLineNumber.activeForeground": "#a8a8a8",
		"editorLink.activeForeground": "#88aacc",
		"diffEditor.insertedLineBackground": "#dfefd8ee",

		// other ui
		"activityBar.background": "#303030",
		"activityBar.foreground": "#cccccc",
    "activityBar.activeBorder": "#303030", 
